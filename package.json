{
  "name": "best-behavior",
  "version": "1.0.0",
  "description": "Runner for esbehavior behaviors",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npm run build:runner && npm run build:adapter",
    "build:runner": "tsc --build",
    "build:adapter": "node ./scripts/build.js",
    "dev": "ts-node --swc --esm ./runner/src/index.ts",
    "test": "npm run build:adapter && ts-node --swc --esm ./test/runner.ts"
  },
  "type": "module",
  "author": "Brian Watkins",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.94",
    "@types/firstline": "^2.0.3",
    "great-expectations": "^1.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "firstline": "^2.0.2",
    "glob": "^10.3.10",
    "playwright": "^1.39.0",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "esbehavior": "^4.2.1"
  }
}
